{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/analog-nitro/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "node_modules/@analogjs/nitro",
        "main": "packages/analog-nitro/src/index.ts",
        "tsConfig": "packages/analog-nitro/tsconfig.lib.json",
        "assets": ["packages/analog-nitro/*.md"]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/analog-nitro/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/packages/analog-nitro"],
      "options": {
        "jestConfig": "packages/analog-nitro/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
